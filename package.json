{
	"name": "readme",
	"displayName": "readme",
	"description": "readme ",
	"publisher": "ganlinzhen",
	"token": "xeyiy26h5srvmxsuiqrww27dtsbj5h4li7kp6byh6q4lsljvajxa",
	"repository": "https://github.com/ganlinzhen/vscode-plugin-reamde-js.git",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"main": "./src/extension.js",
	"activationEvents": [
		"onCommand:readme.helloWorld"
	],
	"contributes": {
		"commands": [
			{
				"command": "readme.helloWorld",
				"title": "Hello World"
			}
		],
		"keybindings": [
			{
				"command": "readme.helloWorld",
				"key": "ctrl+f10",
				"mac": "cmd+f10",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/title/context": [
				{
					"when": "editorFocus",
					"command": "readme.helloWorld",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"vscode-uri": "^2.1.2"
	}
}
